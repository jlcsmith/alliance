        <xs:attribute ref="gco:nilReason"/>
    </xs:complexType>
    <!-- ========================================================================== -->
    <xs:element name="MGMP_DataProductSpecCode" type="gco:CodeListValue_Type"
                substitutionGroup="gco:CharacterString"/>
    <!-- ........................................................................ -->
    <xs:complexType name="MGMP_DataProductSpecCode_PropertyType">
        <xs:sequence minOccurs="0">
            <xs:element ref="mgmp:MGMP_DataProductSpecCode"/>
        </xs:sequence>
        <xs:attribute ref="gco:nilReason"/>
    </xs:complexType>
    <!-- ========================================================================== -->
    <xs:element name="MGMP_FormatCode" type="gco:CodeListValue_Type"
                substitutionGroup="gco:CharacterString"/>
    <!-- ........................................................................ -->
    <xs:complexType name="MGMP_FormatCode_PropertyType">
        <xs:sequence minOccurs="0">
            <xs:element ref="mgmp:MGMP_FormatCode"/>
        </xs:sequence>
        <xs:attribute ref="gco:nilReason"/>
    </xs:complexType>
    <!-- ========================================================================== -->
    <!-- MGMP Spatial Reference System Extensions -->
    <!-- ========================================================================== -->
    <xs:complexType name="MGMP_ReferenceSystem_Type">
        <xs:annotation>
            <xs:documentation>Extension that supports explicit encoding of spatial/coordinate
                reference system (CRS) metadata, incuding component information (ellipsoid, datum,
                projection), grids/graticules, and full inline specification of CRS-related objects
                using OGC Well-Known-Text (WKT) encoding format.
            </xs:documentation>
            <xs:documentation>NOTE: The encoding supports one of i) crs, ii) grid or iii)
                {ellipsoid, datum, projection}. In case iii) each component is optional, however
                there are business rules suggesting the ellipsoid and/or datum should always be
                present.
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="gmd:MD_ReferenceSystem_Type">
                <xs:choice>
                    <xs:element name="crs" type="mgmp:MGMP_CRS_Object_PropertyType"/>
                    <xs:element name="grid" type="mgmp:MGMP_CRS_Object_PropertyType"/>
                    <xs:sequence>
                        <xs:element name="ellipsoid" type="mgmp:MGMP_CRS_Object_PropertyType"
                                    minOccurs="0"/>
                        <xs:element name="datum" type="mgmp:MGMP_CRS_Object_PropertyType"
                                    minOccurs="0"/>
                        <xs:element name="projection" type="mgmp:MGMP_CRS_Object_PropertyType"
                                    minOccurs="0"/>
                    </xs:sequence>
                </xs:choice>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <!-- ........................................................................ -->
    <xs:element name="MGMP_ReferenceSystem" type="mgmp:MGMP_ReferenceSystem_Type"
                substitutionGroup="gmd:MD_ReferenceSystem"/>
    <!-- ........................................................................ -->
    <xs:complexType name="MGMP_ReferenceSystem_PropertyType">
        <xs:sequence minOccurs="0">
            <xs:element ref="mgmp:MGMP_ReferenceSystem"/>
        </xs:sequence>
        <xs:attributeGroup ref="gco:ObjectReference"/>
        <xs:attribute ref="gco:nilReason"/>
    </xs:complexType>
    <!-- ========================================================================== -->
    <xs:complexType name="MGMP_CRS_Object_Type">
        <xs:annotation>
            <xs:documentation>A CRS or CRS-related object (such as an ellipsoid, datum or
                projection) either identified by external registry/catalogue ID or fully specified
                inline (e.g. using OGC Well-Known-Text (WKT) encoding format).
            </xs:documentation>
            <xs:documentation>NOTE: both encodings may be used when the metadata producer wishes to
                document the object via its ID but also provide an inline specification. EXAMPLE:
                identify the projection via a well-known code (such as UK-DIGEST) but also document
                details of the projection parameters (such as false easting/northing) using WKT.
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="gco:AbstractObject_Type">
                <xs:sequence>
                    <xs:element name="identifier" type="gmd:RS_Identifier_PropertyType"
                                minOccurs="0"/>
                    <xs:element name="specification" type="mgmp:MGMP_CRS_Specification_PropertyType"
                                minOccurs="0"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <!-- ........................................................................ -->
    <xs:element name="MGMP_CRS_Object" type="mgmp:MGMP_CRS_Object_Type"/>
    <!-- ........................................................................ -->
    <xs:complexType name="MGMP_CRS_Object_PropertyType">
        <xs:sequence minOccurs="0">
            <xs:element ref="mgmp:MGMP_CRS_Object"/>
        </xs:sequence>
        <xs:attributeGroup ref="gco:ObjectReference"/>
        <xs:attribute ref="gco:nilReason"/>
    </xs:complexType>
    <!-- ========================================================================== -->
    <xs:complexType name="MGMP_CRS_Identifier_Type">
        <xs:annotation>
            <xs:documentation>Extension of RS_Identifier to allow a human readable ‘label’ to be
                documented.
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="gmd:RS_Identifier_Type">
                <xs:sequence>
                    <xs:element name="label" type="gco:CharacterString_PropertyType" minOccurs="0"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <!-- ........................................................................ -->
    <xs:element name="MGMP_CRS_Identifier" type="mgmp:MGMP_CRS_Identifier_Type"
                substitutionGroup="gmd:RS_Identifier"/>
    <!-- ........................................................................ -->
    <xs:complexType name="MGMP_CRS_Identifier_PropertyType">
        <xs:sequence minOccurs="0">
            <xs:element ref="mgmp:MGMP_CRS_Identifier"/>
        </xs:sequence>
        <xs:attributeGroup ref="gco:ObjectReference"/>
        <xs:attribute ref="gco:nilReason"/>
    </xs:complexType>
    <!-- ========================================================================== -->
    <xs:complexType name="MGMP_AbstractCRS_Specification_Type" abstract="true">
        <xs:annotation>
            <xs:documentation>Abstract type for inline specification of CRS or CRS-related
                objects.
            </xs:documentation>
            <xs:documentation>NOTE: concrete sub-types can be defined to support specific encodings
                such as OGC WKT or GML. In the present version of MGMP only WKT is supported.
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="gco:AbstractObject_Type">
                <xs:sequence>
                    <xs:element name="label" type="gco:CharacterString_PropertyType" minOccurs="0"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <!-- ........................................................................ -->
    <xs:element name="MGMP_AbstractCRS_Specification"
                type="mgmp:MGMP_AbstractCRS_Specification_Type" abstract="true"/>
    <!-- ........................................................................ -->
    <xs:complexType name="MGMP_CRS_Specification_PropertyType">
        <xs:sequence minOccurs="0">
            <xs:element ref="mgmp:MGMP_AbstractCRS_Specification"/>
        </xs:sequence>
        <xs:attributeGroup ref="gco:ObjectReference"/>
        <xs:attribute ref="gco:nilReason"/>
    </xs:complexType>
    <!-- ========================================================================== -->
    <xs:complexType name="MGMP_CRS_WKTSpecification_Type">
        <xs:annotation>
            <xs:documentation>Specification of CRS or CRS-related objects using OGC Well-Known-Text
                (WKT) encoding format.
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="mgmp:MGMP_AbstractCRS_Specification_Type">
                <xs:sequence>
                    <xs:element name="wkt" type="gco:CharacterString_PropertyType"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <!-- ........................................................................ -->
    <xs:element name="MGMP_CRS_WKTSpecification" type="mgmp:MGMP_CRS_WKTSpecification_Type"
                substitutionGroup="mgmp:MGMP_AbstractCRS_Specification"/>
    <!-- ........................................................................ -->
    <xs:complexType name="MGMP_CRS_WKTSpecification_PropertyType">
        <xs:sequence minOccurs="0">
            <xs:element ref="mgmp:MGMP_CRS_WKTSpecification"/>
        </xs:sequence>
        <xs:attributeGroup ref="gco:ObjectReference"/>
        <xs:attribute ref="gco:nilReason"/>
    </xs:complexType>
    <!-- ========================================================================== -->
    <!-- MGMP Lineage Extensions -->
    <!-- ========================================================================== -->
    <xs:complexType name="MGMP_Source_Type">
        <xs:annotation>
            <xs:documentation>Extends LI_Source to support multiple spatial/coordinate reference
                system (CRS) metadata.
            </xs:documentation>
            <xs:documentation>NOTE: The ISO element LI_Source allows encoding of only one CRS
                element. MGMP needs to support many CRS components. The ISO model has been extended
                to be consistent with the encoding of CRS elements on the top-level MD_Metadata
                element.
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="gmd:LI_Source_Type">
                <xs:sequence>
                    <xs:element name="sourceReferenceSystem"
                                type="gmd:MD_ReferenceSystem_PropertyType" minOccurs="0"
                                maxOccurs="unbounded"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <!-- ........................................................................ -->
    <xs:element name="MGMP_Source" type="mgmp:MGMP_Source_Type" substitutionGroup="gmd:LI_Source"/>
    <!-- ........................................................................ -->
    <xs:complexType name="MGMP_Source_PropertyType">
        <xs:sequence minOccurs="0">
            <xs:element ref="mgmp:MGMP_Source"/>
        </xs:sequence>
        <xs:attributeGroup ref="gco:ObjectReference"/>
        <xs:attribute ref="gco:nilReason"/>
    </xs:complexType>
    <!-- ========================================================================== -->
    <!-- MGMP Quality Extensions -->
    <!-- ========================================================================== -->
    <xs:complexType name="MGMP_DescriptiveResult_Type">
        <xs:annotation>
            <xs:documentation>Used to document a non-quantitative evaluation result for a data
                quality element, expressed using a textual statement.
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="gmd:AbstractDQ_Result_Type">
                <xs:sequence>
                    <xs:element name="statement" type="gco:CharacterString_PropertyType"/>
                    <xs:element name="reportOnlineLocation" type="gmd:URL_PropertyType"
                                minOccurs="0" maxOccurs="unbounded"/>
                    <xs:element name="reportReference" type="gmd:CI_Citation_PropertyType"
                                minOccurs="0" maxOccurs="unbounded"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <!-- ........................................................................ -->
    <xs:element name="MGMP_DescriptiveResult" type="mgmp:MGMP_DescriptiveResult_Type"
                substitutionGroup="gmd:AbstractDQ_Result"/>
    <!-- ........................................................................ -->
    <xs:complexType name="MGMP_DescriptiveResult_PropertyType">
        <xs:sequence minOccurs="0">
            <xs:element ref="mgmp:MGMP_DescriptiveResult"/>
        </xs:sequence>
        <xs:attributeGroup ref="gco:ObjectReference"/>
        <xs:attribute ref="gco:nilReason"/>
    </xs:complexType>
    <!-- ========================================================================== -->
    <xs:complexType name="MGMP_UsabilityElement_Type">
        <xs:annotation>
            <xs:documentation>Data quality element used to measure degree of adherence of a dataset
                to a specific set of requirements.
            </xs:documentation>
            <xs:documentation>Particularly useful when several quality results for different data
                quality elements are aggregated into a single result; for expressing conformance to
                a data product specification; or when summarising the results of a data quality
                report.
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="gmd:AbstractDQ_Element_Type">
                <xs:sequence>
                    <xs:element name="authority" type="gmd:CI_ResponsibleParty_PropertyType"
                                minOccurs="0"/>
                    <xs:element name="reason" type="gco:CharacterString_PropertyType"
                                minOccurs="0"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <!-- ........................................................................ -->
    <xs:element name="MGMP_UsabilityElement" type="mgmp:MGMP_UsabilityElement_Type"
                substitutionGroup="gmd:AbstractDQ_Element"/>
    <!-- ........................................................................ -->
    <xs:complexType name="MGMP_UsabilityElement_PropertyType">
        <xs:sequence minOccurs="0">
            <xs:element ref="mgmp:MGMP_UsabilityElement"/>
        </xs:sequence>
        <xs:attributeGroup ref="gco:ObjectReference"/>
        <xs:attribute ref="gco:nilReason"/>
    </xs:complexType>
    <!-- ========================================================================== -->
    <xs:complexType name="MGMP_ResourceGradeCode_Type" mixed="true">
        <xs:annotation>
            <xs:documentation>Used to document Resource Grade: a quality grade that provides a high
                level assessment of the resource in terms of content, coverage, and/or currency.
                Values are defined by the codelist MGMP_ResourceGradeCode.
            </xs:documentation>
            <xs:documentation>Intended to be used to encode the value of a
                gmd:DQ_QuantitativeResult. Implemented as a codelist value, based on the ISO 19139
                type gco:CodeListValue_Type, but optimised to be used with gco:Record.
            </xs:documentation>
        </xs:annotation>
        <xs:attribute name="codeList" type="xs:anyURI" use="required"/>
        <xs:attribute name="codeListValue" type="xs:anyURI" use="required"/>
        <xs:attribute name="codeSpace" type="xs:anyURI"/>
    </xs:complexType>
</xs:schema>
