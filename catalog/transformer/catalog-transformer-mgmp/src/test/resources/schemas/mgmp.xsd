           targetNamespace="http://mod.uk/spatial/ns/mgmp/2.0" elementFormDefault="qualified"
           version="2.0">
    <!-- ================================= Annotation ================================ -->
    <xs:annotation>
        <xs:documentation>DEFSTAN 00-103 - MOD Geospatial Metadata Profile (MGMP) Part 2 - XML
