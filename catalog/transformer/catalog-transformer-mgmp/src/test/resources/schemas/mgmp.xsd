        </xs:documentation>
        <xs:documentation>XML Schema for MGMP profile of ISO 19115:2003 Cor 2006, ISO 19119:2005
            Amd.1:2008. Encoded according to ISO 19139:2007.
        </xs:documentation>
        <xs:documentation>Joint User IS-Standards, 2016-01-21</xs:documentation>
        <xs:documentation>Created by C. Davison (Raytheon) and N. Kirk (Envitia) on behalf of UK
            MOD.
        </xs:documentation>
    </xs:annotation>
    <!-- ================================== Imports ================================== -->
    <xs:import namespace="http://www.isotc211.org/2005/gmd"
               schemaLocation="iso/19139/20070417/gmd/gmd.xsd"/>
    <xs:import namespace="http://www.isotc211.org/2005/gco"
               schemaLocation="iso/19139/20070417/gco/gco.xsd"/>
    <xs:import namespace="http://www.isotc211.org/2005/gss"
               schemaLocation="iso/19139/20070417/gss/gss.xsd"/>
    <xs:import namespace="http://www.isotc211.org/2005/gts"
               schemaLocation="iso/19139/20070417/gts/gts.xsd"/>
    <xs:import namespace="http://www.isotc211.org/2005/srv"
               schemaLocation="iso/19139/20060504/srv/srv.xsd"/>
    <xs:import namespace="http://www.opengis.net/gml/3.2" schemaLocation="gml/3.2.1/gml.xsd"/>
    <!-- ########################################################################### -->
    <!-- ########################################################################### -->
    <!-- ================================== Classes ================================= -->
    <!-- ========================================================================== -->
    <!-- MGMP Security Constraints Extensions -->
    <!-- ========================================================================== -->
    <xs:complexType name="MGMP_SecurityConstraints_Type">
        <xs:annotation>
            <xs:documentation>Security constraints for metadata or resource.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="gmd:MD_SecurityConstraints_Type">
                <xs:sequence>
                    <xs:element name="originatorClassification"
                                type="gmd:MD_ClassificationCode_PropertyType" minOccurs="0"/>
                    <xs:element name="caveat" type="gco:CharacterString_PropertyType"
                                minOccurs="0"/>
                    <xs:element name="codeword" type="gco:CharacterString_PropertyType"
                                minOccurs="0" maxOccurs="unbounded"/>
                    <xs:element name="nonUKCodeword" type="gco:CharacterString_PropertyType"
                                minOccurs="0" maxOccurs="unbounded"/>
                    <xs:element name="compartment" type="gco:CharacterString_PropertyType"
                                minOccurs="0" maxOccurs="unbounded"/>
                    <xs:element name="STRAP" type="gco:CharacterString_PropertyType" minOccurs="0"/>
                    <xs:element name="specialHandling" type="gco:CharacterString_PropertyType"
                                minOccurs="0" maxOccurs="unbounded"/>
                    <xs:element name="markingChange" type="mgmp:MGMP_MarkingChange_PropertyType"
                                minOccurs="0" maxOccurs="unbounded"/>
                    <xs:element name="timeValidityForAccessControlMarkings"
                                type="mgmp:MGMP_SecurityTimeValidity_PropertyType" minOccurs="0"
                                maxOccurs="unbounded"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <!-- ........................................................................ -->
    <xs:element name="MGMP_SecurityConstraints" type="mgmp:MGMP_SecurityConstraints_Type"
                substitutionGroup="gmd:MD_SecurityConstraints"/>
    <!-- ........................................................................ -->
    <xs:complexType name="MGMP_SecurityConstraints_PropertyType">
        <xs:sequence minOccurs="0">
            <xs:element ref="mgmp:MGMP_SecurityConstraints"/>
        </xs:sequence>
        <xs:attributeGroup ref="gco:ObjectReference"/>
        <xs:attribute ref="gco:nilReason"/>
    </xs:complexType>
    <!-- ========================================================================== -->
    <xs:complexType name="MGMP_SecurityTimeValidity_Type">
        <xs:annotation>
            <xs:documentation>Expiry time for classification.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="oldClassification" type="gmd:MD_ClassificationCode_PropertyType"/>
            <xs:element name="newClassification" type="gmd:MD_ClassificationCode_PropertyType"/>
            <xs:element name="expireDate" type="gco:Date_PropertyType"/>
        </xs:sequence>
    </xs:complexType>
    <!-- ........................................................................ -->
    <xs:element name="MGMP_SecurityTimeValidity" type="mgmp:MGMP_SecurityTimeValidity_Type"/>
    <!-- ........................................................................ -->
    <xs:complexType name="MGMP_SecurityTimeValidity_PropertyType">
        <xs:sequence minOccurs="0">
            <xs:element ref="mgmp:MGMP_SecurityTimeValidity"/>
        </xs:sequence>
        <xs:attributeGroup ref="gco:ObjectReference"/>
        <xs:attribute ref="gco:nilReason"/>
    </xs:complexType>
    <!-- ========================================================================== -->
    <xs:complexType name="MGMP_MarkingChange_Type">
        <xs:annotation>
            <xs:documentation>Classification or caveat change log.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="previousClassification"
                        type="gmd:MD_ClassificationCode_PropertyType"/>
            <xs:element name="previousCaveat" type="gco:CharacterString_PropertyType"
                        minOccurs="0"/>
            <xs:element name="changeDateTime" type="gco:DateTime_PropertyType"/>
            <xs:element name="authority" type="gmd:CI_ResponsibleParty_PropertyType"/>
        </xs:sequence>
    </xs:complexType>
    <!-- ........................................................................ -->
    <xs:element name="MGMP_MarkingChange" type="mgmp:MGMP_MarkingChange_Type"/>
    <!-- ........................................................................ -->
    <xs:complexType name="MGMP_MarkingChange_PropertyType">
        <xs:sequence minOccurs="0">
            <xs:element ref="mgmp:MGMP_MarkingChange"/>
        </xs:sequence>
        <xs:attributeGroup ref="gco:ObjectReference"/>
        <xs:attribute ref="gco:nilReason"/>
    </xs:complexType>
    <!-- ========================================================================== -->
    <!-- MGMP Constraints, Releasability and Designation Extensions -->
    <!-- ========================================================================== -->
    <xs:complexType name="MGMP_Constraints_Type">
        <xs:annotation>
            <xs:documentation>Information about releasability constraints and designation.
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="gmd:MD_Constraints_Type">
                <xs:sequence>
                    <xs:element name="designation" type="mgmp:MGMP_Designation_PropertyType"
                                minOccurs="0" maxOccurs="unbounded"/>
                    <xs:element name="releasability" type="mgmp:MGMP_Releasability_PropertyType"
                                minOccurs="0" maxOccurs="unbounded"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <!-- ........................................................................ -->
    <xs:element name="MGMP_Constraints" type="mgmp:MGMP_Constraints_Type"
                substitutionGroup="gmd:MD_Constraints"/>
    <!-- ........................................................................ -->
    <xs:complexType name="MGMP_Constraints_PropertyType">
        <xs:sequence minOccurs="0">
