        <xs:attributeGroup ref="gco:ObjectReference"/>
        <xs:attribute ref="gco:nilReason"/>
    </xs:complexType>
    <!-- ========================================================================== -->
    <xs:complexType name="MGMP_Releasability_Type">
        <xs:annotation>
            <xs:documentation>Releasability to NATO, defence, and other organisations.
