            <xs:element name="version" type="gco:CharacterString_PropertyType" minOccurs="0"/>
            <xs:element name="abstract" type="gco:CharacterString_PropertyType" minOccurs="0"/>
            <xs:element name="purpose" type="gco:CharacterString_PropertyType" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>
    <!-- ........................................................................ -->
    <xs:element name="MGMP_DataProductSpecification"
                type="mgmp:MGMP_DataProductSpecification_Type"/>
    <!-- ........................................................................ -->
    <xs:complexType name="MGMP_DataProductSpecification_PropertyType">
        <xs:sequence minOccurs="0">
            <xs:element ref="mgmp:MGMP_DataProductSpecification"/>
        </xs:sequence>
