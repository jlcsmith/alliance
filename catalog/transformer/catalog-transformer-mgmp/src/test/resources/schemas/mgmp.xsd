            <xs:element name="specificReleasability" type="gco:CharacterString_PropertyType"
                        minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>
    <!-- ........................................................................ -->
    <xs:element name="MGMP_Releasability" type="mgmp:MGMP_Releasability_Type"/>
    <!-- ........................................................................ -->
    <xs:complexType name="MGMP_Releasability_PropertyType">
        <xs:sequence minOccurs="0">
            <xs:element ref="mgmp:MGMP_Releasability"/>
        </xs:sequence>
        <xs:attributeGroup ref="gco:ObjectReference"/>
        <xs:attribute ref="gco:nilReason"/>
    </xs:complexType>
    <!-- ========================================================================== -->
    <xs:element name="NGMP_AccessibilityLevelCode" type="gco:CodeListValue_Type"
                substitutionGroup="gco:CharacterString"/>
    <!-- ........................................................................ -->
    <xs:complexType name="NGMP_AccessibilityLevelCode_PropertyType">
        <xs:sequence minOccurs="0">
            <xs:element ref="mgmp:NGMP_AccessibilityLevelCode"/>
        </xs:sequence>
        <xs:attribute ref="gco:nilReason"/>
    </xs:complexType>
    <!-- ........................................................................ -->
    <!-- ========================================================================== -->
    <xs:element name="MGMP_UKReleasabilityCode" type="gco:CodeListValue_Type"
                substitutionGroup="gco:CharacterString"/>
    <!-- ........................................................................ -->
    <xs:complexType name="MGMP_UKReleasabilityCode_PropertyType">
        <xs:sequence minOccurs="0">
            <xs:element ref="mgmp:MGMP_UKReleasabilityCode"/>
        </xs:sequence>
        <xs:attribute ref="gco:nilReason"/>
    </xs:complexType>
    <!-- ========================================================================== -->
    <xs:complexType name="MGMP_Designation_Type">
        <xs:annotation>
            <xs:documentation>Constraints covering authorisation for use of a product for a specific
                exercise or operation, either as the designated or supplementary resource.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="designationAuthority" type="gmd:CI_ResponsibleParty_PropertyType"/>
            <xs:element name="designationDate" type="gco:DateTime_PropertyType"/>
            <xs:element name="designationType" type="mgmp:MGMP_DesignationTypeCode_PropertyType"/>
            <xs:element name="operationOrExerciseName" type="gco:CharacterString_PropertyType"
                        minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>
    <!-- ........................................................................ -->
    <xs:element name="MGMP_Designation" type="mgmp:MGMP_Designation_Type"/>
    <!-- ........................................................................ -->
    <xs:complexType name="MGMP_Designation_PropertyType">
        <xs:sequence minOccurs="0">
            <xs:element ref="mgmp:MGMP_Designation"/>
        </xs:sequence>
        <xs:attributeGroup ref="gco:ObjectReference"/>
        <xs:attribute ref="gco:nilReason"/>
    </xs:complexType>
    <!-- ========================================================================== -->
    <xs:element name="MGMP_DesignationTypeCode" type="gco:CodeListValue_Type"
                substitutionGroup="gco:CharacterString"/>
    <!-- ........................................................................ -->
    <xs:complexType name="MGMP_DesignationTypeCode_PropertyType">
        <xs:sequence minOccurs="0">
            <xs:element ref="mgmp:MGMP_DesignationTypeCode"/>
        </xs:sequence>
        <xs:attribute ref="gco:nilReason"/>
    </xs:complexType>
    <!-- ========================================================================== -->
    <!-- MGMP Extent Extensions -->
    <!-- ========================================================================== -->
    <xs:complexType name="MGMP_VerticalExtent_Type">
        <xs:annotation>
            <xs:documentation>Extends the class MD_VerticalExtent from ISO to include location of
                lowest and highest elevation.
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="gmd:EX_VerticalExtent_Type">
                <xs:sequence>
                    <xs:element name="minElevationPosition" type="gml:PointPropertyType"
                                minOccurs="0" maxOccurs="unbounded"/>
                    <xs:element name="maxElevationPosition" type="gml:PointPropertyType"
                                minOccurs="0" maxOccurs="unbounded"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <!-- ........................................................................ -->
    <xs:element name="MGMP_VerticalExtent" type="mgmp:MGMP_VerticalExtent_Type"
                substitutionGroup="gmd:EX_VerticalExtent"/>
    <!-- ........................................................................ -->
    <xs:complexType name="MGMP_VerticalExtent_PropertyType">
        <xs:sequence minOccurs="0">
            <xs:element ref="mgmp:MGMP_VerticalExtent"/>
        </xs:sequence>
        <xs:attributeGroup ref="gco:ObjectReference"/>
        <xs:attribute ref="gco:nilReason"/>
    </xs:complexType>
    <!-- ========================================================================== -->
    <!-- MGMP Language Extensions -->
    <!-- ========================================================================== -->
    <xs:complexType name="MGMP_LanguageCode_Type">
        <xs:annotation>
            <xs:documentation>Language code with added script and component attributes.
            </xs:documentation>
            <xs:documentation>script: documents the script used with the language. For example,
                Russian with Cyrillic script or Russian with Roman (transliterated) script. The
                value shall be from the codelist MGMP_LanguageScriptCode (this rule is not enforced
                by the XML Schema).
            </xs:documentation>
            <xs:documentation>component: describes the component of the dataset to which the
                language applies. For example, the language may only apply to the map legend,
                marginalia, main panel or insets. The value shall be from the codelist
                MGMP_LanguageComponentCode (this rule is not enforced by the XML Schema).
            </xs:documentation>
        </xs:annotation>
        <xs:simpleContent>
            <xs:extension base="gco:CodeListValue_Type">
                <xs:attribute ref="mgmp:script"/>
                <xs:attribute ref="mgmp:component"/>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>
    <!-- ........................................................................ -->
    <xs:attribute name="script" type="xs:string">
        <!--
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:enumeration value="cyrillic"/>
                    <xs:enumeration value="roman"/>
                </xs:restriction>
            </xs:simpleType>
        -->
    </xs:attribute>
    <!-- ........................................................................ -->
    <xs:attribute name="component" type="xs:string">
        <!--
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:enumeration value="mapFace"/>
                    <xs:enumeration value="legend"/>
                </xs:restriction>
            </xs:simpleType>
        -->
    </xs:attribute>
    <!-- ........................................................................ -->
    <xs:element name="MGMP_LanguageCode" type="mgmp:MGMP_LanguageCode_Type"
                substitutionGroup="gco:CharacterString"/>
    <!-- ........................................................................ -->
    <xs:complexType name="MGMP_LanguageCode_PropertyType">
        <xs:sequence minOccurs="0">
            <xs:element ref="mgmp:MGMP_LanguageCode"/>
        </xs:sequence>
        <xs:attribute ref="gco:nilReason"/>
    </xs:complexType>
    <!-- ========================================================================== -->
    <!-- MGMP Content Information Extensions -->
    <!-- ========================================================================== -->
    <xs:complexType name="MGMP_ImageDescription_Type">
        <xs:annotation>
            <xs:documentation>Information about an image's suitability for use.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="gmd:MD_ImageDescription_Type">
                <xs:sequence>
                    <xs:element name="lens" type="gco:CharacterString_PropertyType" minOccurs="0"/>
                    <xs:element name="frameNumber" type="gmd:RS_Identifier_PropertyType"
                                minOccurs="0"/>
                    <xs:element name="recordNumber" type="gmd:RS_Identifier_PropertyType"
                                minOccurs="0"/>
                    <xs:element name="comments" type="gco:CharacterString_PropertyType"
                                minOccurs="0"/>
                    <xs:element name="recordComplete" type="gco:Boolean_PropertyType"
                                minOccurs="0"/>
                    <xs:element name="description" type="gco:CharacterString_PropertyType"
                                minOccurs="0"/>
                    <xs:element name="intelValue" type="gco:Boolean_PropertyType" minOccurs="0"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <!-- ........................................................................ -->
    <xs:element name="MGMP_ImageDescription" type="mgmp:MGMP_ImageDescription_Type"
                substitutionGroup="gmd:MD_ImageDescription"/>
    <!-- ........................................................................ -->
    <xs:complexType name="MGMP_ImageDescription_PropertyType">
        <xs:sequence minOccurs="0">
            <xs:element ref="mgmp:MGMP_ImageDescription"/>
        </xs:sequence>
        <xs:attributeGroup ref="gco:ObjectReference"/>
        <xs:attribute ref="gco:nilReason"/>
    </xs:complexType>
    <!-- ========================================================================== -->
    <xs:complexType name="MGMP_VideoDescription_Type">
        <xs:annotation>
            <xs:documentation>Information about an video's suitability for use.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="gmd:MD_CoverageDescription_Type">
                <xs:sequence>
                    <xs:element name="tapeId" type="gmd:RS_Identifier_PropertyType" minOccurs="0"/>
                    <xs:element name="clipDuration" type="gts:TM_PeriodDuration_PropertyType"
                                minOccurs="0"/>
                    <xs:element name="logSheetNumber" type="gmd:RS_Identifier_PropertyType"
                                minOccurs="0"/>
                    <xs:element name="recordNumber" type="gmd:RS_Identifier_PropertyType"
                                minOccurs="0"/>
                    <xs:element name="comments" type="gco:CharacterString_PropertyType"
                                minOccurs="0"/>
                    <xs:element name="recordComplete" type="gco:Boolean_PropertyType"
                                minOccurs="0"/>
                    <xs:element name="description" type="gco:CharacterString_PropertyType"
                                minOccurs="0"/>
                    <xs:element name="intelValue" type="gco:Boolean_PropertyType" minOccurs="0"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <!-- ........................................................................ -->
    <xs:element name="MGMP_VideoDescription" type="mgmp:MGMP_VideoDescription_Type"
