    <!-- ========================================================================== -->
    <!-- MGMP Series and Data Product Specification Extensions -->
    <!-- ========================================================================== -->
    <xs:complexType name="MGMP_Series_Type">
        <xs:annotation>
            <xs:documentation>Extends CI_Series to provide information about the data product
                specification associated with the series.
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="gmd:CI_Series_Type">
                <xs:sequence>
                    <xs:element name="dataProductSpecification"
                                type="mgmp:MGMP_DataProductSpecification_PropertyType"
                                minOccurs="0"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <!-- ........................................................................ -->
    <xs:element name="MGMP_Series" type="mgmp:MGMP_Series_Type" substitutionGroup="gmd:CI_Series"/>
    <!-- ........................................................................ -->
    <xs:complexType name="MGMP_Series_PropertyType">
        <xs:sequence minOccurs="0">
            <xs:element ref="mgmp:MGMP_Series"/>
        </xs:sequence>
        <xs:attributeGroup ref="gco:ObjectReference"/>
        <xs:attribute ref="gco:nilReason"/>
    </xs:complexType>
    <!-- ========================================================================== -->
    <xs:complexType name="MGMP_DataProductSpecification_Type">
        <xs:annotation>
            <xs:documentation>Information about the data product specification to which the resource
